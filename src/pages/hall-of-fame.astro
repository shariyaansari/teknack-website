---
import Header from "../components/Header.astro";
import Preloader from "../components/Preloader.astro";
import Navbar from "../components/Navbar.astro";
import InnerPageHero from "../components/InnerPageHero.astro";
import Footer from "../components/Footer.astro";
import AwardsGrid from "../components/AwardsGrid.astro";
import awardsData from "../data/awardsData.js";

const title = "Hall of Fame";
---

<!doctype html>
<html lang="en">
  <head>
    <Header title={title} />
  </head>

  <body>
    <Preloader />
    <Navbar />
    <InnerPageHero title={title} />
    <AwardsGrid title="Teknack 2024" awardsData={awardsData["2024"]} />
    <AwardsGrid title="Teknack 2023" awardsData={awardsData["2023"]} />
    <AwardsGrid title="Teknack 2022" awardsData={awardsData["2022"]} />
    <AwardsGrid title="Teknack 2021" awardsData={awardsData["2021"]} />
    <AwardsGrid title="Teknack 2020" awardsData={awardsData["2020"]} />
    <AwardsGrid title="Teknack 2019" awardsData={awardsData["2019"]} />

    <Footer />
  </body>
</html>
